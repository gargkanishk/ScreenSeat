{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "name": "vuejs: chrome",
      "url": "http://localhost:8080", // Change this to the correct URL if needed
      "webRoot": "${workspaceFolder}/ticket_booking",
      "runtimeArgs": ["--inspect-brk"],
      "sourceMapPathOverrides": {
        "webpack:///src/*": "${webRoot}/src/*"
      }
    }
  ]
}

  

